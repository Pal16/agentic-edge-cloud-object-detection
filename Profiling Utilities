import numpy as np


def compute_fps(latencies):
    if not latencies:
        return 0.0
    return 1.0 / np.mean(latencies)


def latency_stats(latencies):
    return {
        "mean": float(np.mean(latencies)),
        "p95": float(np.percentile(latencies, 95)),
        "max": float(np.max(latencies))
    }
